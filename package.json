{
  "name": "rentify-backend",
  "version": "1.0.0",
  "description": "Rentify is a tech rental platform that allows users to rent electronic devices like mobile phones, laptops, tablets, cameras, gaming consoles, and accessories for short-term or long-term use.",
  "homepage": "https://github.com/ashish11parmar/Rentify-Backend#readme",
  "bugs": {
    "url": "https://github.com/ashish11parmar/Rentify-Backend/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashish11parmar/Rentify-Backend.git"
  },
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "server.js",
  "scripts": {
    "postman": "node scripts/generate-postman-from-routes.js",
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch --verbose",
    "test:unit": "jest --testNamePattern='Unit Tests' --coverage",
    "test:integration": "jest --testNamePattern='Integration' --coverage --runInBand",
    "test:api": "jest --testNamePattern='API' --coverage --runInBand",
    "test:e2e": "jest --testNamePattern='E2E' --coverage --runInBand --detectOpenHandles",
    "test:security": "jest --testNamePattern='Security' --coverage --runInBand",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:api && npm run test:e2e && npm run test:security",
    "test:coverage": "jest --coverage --coverageReporters=html text lcov",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:specific": "jest --testNamePattern",
    "test:failed": "jest --onlyFailures",
    "test:changed": "jest --onlyChanged",
    "perf:load": "k6 run tests/performance/load-test-segments.js",
    "perf:stress": "k6 run tests/performance/stress-test.js",
    "perf:spike": "k6 run tests/performance/spike-test.js",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "coverage:report": "open coverage/index.html"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-winston": "^4.2.0",
    "googleapis": "^168.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "joi": "^18.0.2",
    "joi-to-swagger": "^6.2.0",
    "jsonwebtoken": "^9.0.3",
    "k6": "^0.0.0",
    "mongoose": "^8.19.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.10.7",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "cross-env": "^10.1.0",
    "fs-extra": "^11.3.2",
    "jest": "^29.7.0",
    "mysql2": "^3.15.3",
    "node-mocks-http": "^1.17.2",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "supertest": "^6.3.3",
    "testcontainers": "^11.10.0"
  }
}
